<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.javaee.entity.FoodpackageEntity" table="foodpackage" schema="yummy">
        <id name="packageid" column="packageid"/>
        <property name="restCode" column="restCode"/>
        <property name="name" column="name"/>
        <property name="foodCodes" column="foodCodes"/>
        <property name="foodNums" column="foodNums"/>
        <property name="price" column="price"/>
    </class>
</hibernate-mapping>