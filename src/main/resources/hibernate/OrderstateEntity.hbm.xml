<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.javaee.entity.OrderstateEntity" table="orderstate" schema="yummy">
        <id name="orderid" column="orderid"/>
        <property name="orderstate" column="orderstate"/>
        <property name="paytime" column="paytime"/>
        <property name="canceltime" column="canceltime"/>
        <property name="arrivetime" column="arrivetime"/>
    </class>
</hibernate-mapping>